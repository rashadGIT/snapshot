{
  "name": "snapspot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "setup:local": "./setup-local.sh",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:component": "vitest run tests/component",
    "test:contract": "vitest run tests/contract",
    "test:security": "vitest run tests/security",
    "test:performance": "vitest run tests/performance",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:browser": "playwright test tests/e2e/browser-compatibility.spec.ts",
    "test:e2e:accessibility": "playwright test tests/e2e/accessibility.spec.ts",
    "test:all": "npm run test && npm run test:e2e"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.564.0",
    "@aws-sdk/s3-request-presigner": "^3.564.0",
    "@prisma/client": "^5.14.0",
    "clsx": "^2.1.1",
    "html5-qrcode": "^2.3.8",
    "jose": "^5.3.0",
    "nanoid": "^5.0.7",
    "next": "^14.2.0",
    "qrcode": "^1.5.3",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.0",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.12.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^4.0.15",
    "autoprefixer": "^10.4.0",
    "axe-core": "^4.11.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.1.3",
    "msw": "^2.12.4",
    "postcss": "^8.4.0",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "prisma": "^5.14.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.10.0",
    "typescript": "^5.4.0",
    "vitest": "^4.0.15"
  }
}
